{
    "name": "componofy",
    "version": "1.3.0",
    "description":
        "Web application that helps user merge the user's and public playlists.",
    "engines": {
        "node": ">=8.9.3",
        "npm": ">=5.5.1",
        "yarn": ">=1.3.2"
    },
    "scripts": {
        "build-css":
            "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
        "watch-css":
            "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start-js": "cross-env react-scripts start",
        "start": "yarn start:api",
        "start:dev": "npm-run-all -p watch-css start-js",
        "start:api": "babel-node server.js --presets es2015,stage-2",
        "build": "npm run build-css && react-scripts build",
        "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
        "heroku-postbuild": "yarn install && yarn build",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "classnames": "^2.2.5",
        "dotenv": "^5.0.1",
        "hapi": "^17.1",
        "history": "^4.7.2",
        "inert": "^5.0.1",
        "material-ui": "^1.0.0-beta.40",
        "material-ui-icons": "^1.0.0-beta.15",
        "node-fetch": "^2.1.2",
        "node-sass-chokidar": "^1.2.2",
        "normalizr": "^3.2.4",
        "prop-types": "^15.6.0",
        "ramda": "^0.25.0",
        "react": "^16.0.3",
        "react-beautiful-dnd": "^7.0.0",
        "react-dom": "^16.3.1",
        "react-dropzone": "^4.2.3",
        "react-hotkeys": "^1.1.1",
        "react-icons": "^2.2.5",
        "react-media-player": "^0.7.0",
        "react-popper": "^0.10.1",
        "react-redux": "^5.0.6",
        "react-responsive": "^4.0.4",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.1.2",
        "react-router-redux": "next",
        "react-scripts": "^1.1.4",
        "react-scroll": "^1.6.4",
        "react-waypoint": "^8.0.1",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "spotify-web-api-node": "^3.0.0",
        "yar": "^9.0.1"
    },
    "devDependencies": {
        "enzyme-to-json": "^3.3.3",
        "cross-env": "^5.0.5",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "jest": "^21.2.1",
        "npm-run-all": "^4.0.2",
        "prettier": "^1.7.0",
        "react-test-renderer": "^16.3.1",
        "redux-logger": "^3.0.6"
    },
    "proxy": "http://localhost:3001/",
    "main": "server.js",
    "repository": "https://github.com/DalerAsrorov/componofy.git",
    "author": "Daler Asrorov <asrorids@gmail.com>",
    "license": "Apache-2.0"
}
